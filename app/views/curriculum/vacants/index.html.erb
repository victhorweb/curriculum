<h1>Lista de Vagas</h1>

<table>
  <tr>
    <th>Titulo</th>
    <th>Descrição</th>
    <th>Data para o termino</th>
    <th>Setor da vaga</th>
    <th></th>

  </tr>

<% @vacants.each do |vacant| %>
  <tr>
    <td><%= vacant.title %></td>
    <td><%= vacant.description %></td>
    <td><%= vacant.date_end %></td>
    <td><%= vacant.sector_id %></td>
    <td><%= link_to 'Ver mais', "/curriculum/vacants/show/#{vacant.id}" %></td>

  </tr>
<% end %>
</table>

<br />


